datasource DS {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Menu {
  id        Int      @id @default(autoincrement())
  imageSrc  String
  date      String   @unique
  createdAt DateTime @default(now())
}
